<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- DAO생성 -->
<mapper namespace="com.koreait.spring.user.UserMapper">
    <insert id="insUser">
        INSERT INTO t_user
        (uid, upw, unm, gender)
        VALUES
        (#{uid}, #{upw}, #{unm}, #{gender})
    </insert>
</mapper>

